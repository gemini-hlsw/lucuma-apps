{
  "https_only": true,
  "root": "static/",
  "clean_urls": false,
  "redirects": {
    "/conf.json": {
      "url": "\" /${ENVIRONMENT}.conf.json\""
    }
  },
  "routes": {
    "/*": "/",
    "/**": "index.html"
  },
  "headers": {
    "/": {
      "Cache-Control": "no-store, no-cache"
    },
    "/assets/**": {
      "Cache-Control": "public, max-age=31536000"
    }
  }
}  
